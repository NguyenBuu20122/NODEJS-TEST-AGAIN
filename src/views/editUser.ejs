<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../css/styles.css">
    <title>Update</title>
</head>
<body>
    <div class="topnav">
        <a  href="/">Home</a>
        <a href="/add-new-user">Create User</a>
        <a href="/contact">Contact</a>
        <a href="/about">About</a>
      </div>
      <div class="formadduser">
        
        <form method="post" action="updateuser" >
            <Fieldset class="lguser">
                <legend >Edit-User </legend>
                    <div class="item-user">
                        <label for="email"> Email:</label>
                        <input type="text" name="email" id="" value="<%= inforUser.email%>">
                    </div>
                    <div class="item-user">
                        <label for="name"> NAME:</label>
                        <input type="text" name="name" id="" value="<%= inforUser.name %>">
                    </div>
                    <div class="item-user">
                        <label for="city"> CITY:</label>
                        <input type="text" name="city" id="" value="<%= inforUser.city %>">
                    </div>

                    <div class="item-user">
                        <input type="submit" value="Save">
                    </div>
            </Fieldset>
        </form>
      </div>
</body>
</html>